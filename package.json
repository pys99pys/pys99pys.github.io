{
  "homepage": "https://pys99pys.github.io/blog",
  "name": "blog-client",
  "version": "1.0.0",
  "description": "my tech blog frontend project",
  "scripts": {
    "dev": "NODE_ENV=local webpack-dev-server",
    "build:staging": "export NODE_ENV=staging && rm -rf ./build && webpack",
    "build:production": "export NODE_ENV=production && rm -rf ./build && webpack",
    "predeploy": "yarn build:production",
    "deploy": "gh-pages -d build",
    "eslint": "eslint src/**/*.tsx"
  },
  "author": "pys99pys@gmail.com",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "axios": "^0.19.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.0",
    "react-router-dom": "^5.1.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.1"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.10"
  }
}
